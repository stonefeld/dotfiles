#"pactl set-sink-volume @DEFAULT_SINK@ +10%"
"current=$(pacmd dump-volumes | awk 'NR==1{print $8}' | sed 's/\%//') && [ $current -lt 200 ] && /usr/bin/pactl set-sink-volume @DEFAULT_SINK@ +10% && /usr/bin/canberra-gtk-play -i audio-volume-change"
    XF86AudioRaiseVolume

"/usr/bin/pactl set-sink-volume @DEFAULT_SINK@ -10% && /usr/bin/canberra-gtk-play -i audio-volume-change"
    XF86AudioLowerVolume

"/usr/bin/pactl set-sink-mute @DEFAULT_SINK@ toggle"
    XF86AudioMute

"/usr/bin/xbacklight -inc 10"
    XF86MonBrightnessUp

"/usr/bin/xbacklight -dec 10"
    XF86MonBrightnessDown

"/usr/bin/playerctl --player=spotify play-pause"
    Pause

"/usr/local/bin/st -c PulseMixer -e pulsemixer"
    Shift+Mod4 + p

"/usr/local/bin/st -c Ranger -e ranger"
    Mod4 + e

"~/.local/bin/surfhistory"
    Shift+Mod4 + o

"/usr/local/bin/dmenu_run -c -l 20 -bw 2"
    Mod4 + p

"~/.local/bin/touchpad"
    Shift + F6
