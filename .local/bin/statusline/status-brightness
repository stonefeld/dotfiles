#!/bin/sh

brightness=$(xbacklight -get)

icon=""
case $brightness in
    "0")   icon="";;
    "10")  icon="";;
    "20")  icon="";;
    "30")  icon="";;
    "40")  icon="";;
    "50")  icon="";;
    "60")  icon="";;
    "70")  icon="";;
    "80")  icon="";;
    "90")  icon="";;
    "100") icon="";;
esac

echo "$icon $brightness%"
